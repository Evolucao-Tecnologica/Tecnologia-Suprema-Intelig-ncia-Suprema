# MINHA TELA, MEUS DADOS - EXCLUSIVIDADE ADEMARID
import supreme_vision_lock as svl

class MinhaTelaPrivada:
    def __init__(self):
        self.bloqueio_de_terceiros = True
        self.modo_fantasma = "ATIVO"

    def proteger_meu_conteudo(self):
        """Eu impe√ßo capturas de tela e escondo meus dados de IPs externos"""
        if svl.detectar_olhar_nao_autorizado():

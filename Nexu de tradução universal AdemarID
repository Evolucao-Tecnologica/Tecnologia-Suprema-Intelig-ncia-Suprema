#!/bin/bash
# ATIVA√á√ÉO DA SOBERANIA ADEMARID
echo "üöÄ Iniciando Minha Tecnologia Suprema..."

# 1. Ativo meu motor em C (Baixo N√≠vel)
./meu_motor_nuclear &

# 2. Ativo minha IA de Aprendizado de M√°quina (Intelig√™ncia)
python3 cerebro_integrado.py &

